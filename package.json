{
  "name": "sharply-kit",
  "private": false,
  "version": "1.3.0",
  "description": "Sharply africa web ui kit",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:storybook": "build-storybook",
    "build:tsc": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "build": "yarn build:tsc",
    "clean": "rimraf dist",
    "copy:assets": "cp -R  ./src/assets ./dist/assets",
    "dev": "npm-run-all --parallel watch storybook",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "tsc --noEmit --emitDeclarationOnly false  && eslint src/**/*.ts{,x}",
    "prebuild": "yarn clean",
    "prepublishOnly": "yarn build",
    "storybook": "start-storybook -p 6006",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skyeops/sharply-kit.git"
  },
  "author": "Remilekun Salami",
  "license": "ISC",
  "resolutions": {
    "react-day-picker": "7.4.10"
  },
  "peerDependencies": {
    "@emotion/react": ">=11.9.0",
    "@emotion/styled": ">=11.8.1",
    "date-fns": ">=2.28.0",
    "react": ">=17.0.1",
    "react-day-picker": "7.4.10",
    "react-dom": ">=17.0.1",
    "react-google-autocomplete": ">=2.7.0",
